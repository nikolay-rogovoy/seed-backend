    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strictNullChecks": false,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "tools"
    ]
}
